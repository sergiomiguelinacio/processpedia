<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .requestFolderListContainer {
      width: 100%;
      padding: 8px 8px;
    }
    .folder {
      color: white;
      padding: 3px 3px;
      width: 100%;
      font-size: 12px;
      font-weight: bold;
      border: 0px;
      background: transparent;
      -webkit-border-radius: 5px;
      cursor: pointer;
      margin-bottom: 4px;
    }
    .folderSelected {
      background-color: white;
      color: black;
    }
  </ui:style>
  <g:VerticalPanel addStyleNames="{style.requestFolderListContainer}"
                   ui:field="folderListContainer">
 </g:VerticalPanel>
</ui:UiBinder>